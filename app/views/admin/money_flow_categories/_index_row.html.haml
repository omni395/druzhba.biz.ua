%tr{ id: dom_id(money_flow_category) }
  %td.icon_cell
    = link_to admin_money_flow_category_path(money_flow_category), class: 'd-block', data: { turbo_frame: 'modal_main' } do
      %i.bi.bi-file-text.h3
  %td
    = money_flow_category.id
  %td
    = money_flow_category.title
  %td
    = money_flow_category.description
  %td
    = render Admin::BadgeComponent.new(money_flow_category.flow_i18n, money_flow_category.flow_color)
  %td.icon_cell
    = link_to edit_admin_money_flow_category_path(money_flow_category), class: 'd-block', data: { turbo_frame: "modal_main" } do
      %i.bi.bi-pencil-square.h3